\"mini\"\r\n            type=\"danger\"\r\n            @click=\"handleDelete(scope.$index, scope.row)\"\r\n            >删除</el-button\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-pagination\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"page\"\r\n      :page-size=\"10\"\r\n      align=\"center\"\r\n      layout=\"prev, pager, next, ->, total\"\r\n      :total=\"total\"\r\n      style=\"margin: 10px 0\"\r\n    >\r\n    </el-pagination>\r\n\r\n\r\n\r\n    <hr color='#eee'>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'AnnouncementManage',\r\n  data() {\r\n    return {\r\n      announcement:[],\r\n      page:1,\r\n      total:0,\r\n    }\r\n  },\r\n  mounted(){\r\n    this.getData();\r\n  },\r\n  methods: {\r\n      handleDelete(index, row) {\r\n        // console.log(index, row);\r\n        this.$API.announcement.reqDeleteAnnouncement(row).then(response => {\r\n          this.getData();\r\n          this.$message({\r\n            message: '删除成功',\r\n            type: 'success'\r\n          });\r\n        }).catch(error => {\r\n          alert(error);\r\n        })\r\n      },\r\n      handleCurrentChange(page){\r\n        this.page = page;\r\n        this.getData();\r\n      },\r\n      getData(){\r\n        this.$API.announcement.reqGetAnnouncementList(this.page).then(response => {\r\n          this.announcement = response.data;\r\n          this.total = response.length;\r\n        }).catch(error => {\r\n          alert(error);\r\n        })\r\n      }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]}]}